pipeline {
    agent any
     parameters {
        string(name: 'processName', defaultValue:'', description: 'Name of the process that we need it ID.')
    }
    stages {
        stage('Preparing-git-Process') {
            steps {
                echo "We are looking for ${params.processName} process."
                sh "ps -ef | grep ${params.processName}"
            }
        }
    }
}
